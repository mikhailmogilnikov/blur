{
  "name": "@blur-ui/mesh-gradient",
  "description": "The Mesh Gradient component for React",
  "keywords": [
    "mesh-gradient",
    "react",
    "ui",
    "component"
  ],
  "author": "Mikhail Mogilnikov",
  "version": "1.0.12",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikhailmogilnikov/blur.git",
    "directory": "packages/standalone/mesh-gradient"
  },
  "homepage": "https://github.com/mikhailmogilnikov/blur/tree/master/packages/standalone/mesh-gradient",
  "bugs": {
    "url": "https://github.com/mikhailmogilnikov/blur/issues"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "tsup src/index.ts --format cjs,esm --dts"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "@blur-ui/react-utils": "*",
    "react-intersection-observer": "^9.15.0"
  },
  "devDependencies": {
    "@blur-ui/typescript-config": "workspace:*",
    "@blur-ui/eslint-config": "workspace:*",
    "@blur-ui/tsup-config": "workspace:*",
    "eslint": "^9",
    "tsup": "^8.3.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}
