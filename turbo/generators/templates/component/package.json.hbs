{
  "name": "@blur-ui/{{packageName}}",
  "description": "The {{packageName}} component for React.",
  "keywords": [
    "{{packageName}}",
    "react",
    "ui",
    "component"
  ],
  "author": "Mikhail Mogilnikov",
  "version": "1.0.0",
  "license": "MIT",
  "private": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/mikhailmogilnikov/blur.git",
    "directory": "packages/{{outDir}}/{{packageName}}"
  },
  "homepage": "https://github.com/mikhailmogilnikov/blur/tree/master/packages/{{outDir}}/{{packageName}}",
  "bugs": {
    "url": "https://github.com/mikhailmogilnikov/blur/issues"
  },
